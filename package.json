{
  "name": "ai-agents-cli",
  "version": "1.0.0",
  "description": "A CLI tool with parallel AI agents using TypeScript and Next.js",
  "main": "src/index.ts",
  "bin": {
    "ai-agents-cli": "./bin/cli.js"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:cli": "tsc --project tsconfig.cli.json",
    "build:all": "npm run build && npm run build:cli",
    "start": "next start",
    "lint": "next lint",
    "cli": "ts-node --project tsconfig.cli.json src/cli/index.ts",
    "cli:build": "node dist/cli/index.js",
    "chat": "npm run cli chat",
    "cli:list": "npm run cli list",
    "cli:run": "npm run cli run",
    "cli:parallel": "npm run cli run-parallel",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "ui": "ts-node --project tsconfig.cli.json src/cli/ui/index.tsx",
    "ui:build": "node dist/cli/ui/index.js",
    "start:ui": "npm run ui",
    "modern": "npm run ui"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^0.0.50",
    "@ai-sdk/google": "^0.0.54",
    "@ai-sdk/openai": "^0.0.66",
    "@types/blessed": "^0.1.25",
    "@types/inquirer": "^9.0.7",
    "ai": "^3.4.0",
    "blessed": "^0.1.81",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "chokidar": "^4.0.3",
    "chromadb": "^3.0.10",
    "commander": "^11.1.0",
    "conf": "^12.0.0",
    "globby": "^14.1.0",
    "gradient-string": "^2.0.2",
    "highlight.js": "^11.11.1",
    "ink": "^6.1.0",
    "ink-box": "^1.0.0",
    "ink-divider": "^4.1.1",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-table": "^3.1.0",
    "ink-text-input": "^6.0.0",
    "inquirer": "^9.2.12",
    "keytar": "^7.9.0",
    "marked": "^12.0.2",
    "marked-terminal": "^7.3.0",
    "nanoid": "^5.0.4",
    "next": "14.0.4",
    "ora": "^8.0.1",
    "prismjs": "^1.30.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "readline": "^1.3.0",
    "strip-ansi": "^7.1.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/glob": "^8.1.0",
    "@types/gradient-string": "^1.1.6",
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "postcss": "^8.4.33",
    "ts-node": "^10.9.1",
    "vitest": "^3.2.4"
  }
}
